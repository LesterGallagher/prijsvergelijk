    <%- include('../../_site/onsen_dist/pages.ejs', {}).replace(/<a ([^>]*)>([\s\S]*?)<\/a>/gmi, function(match, attributes, content) {
        var href = attributes.match(/href="(\/[^"]*)"/);
        href = href && href[1];
        var tagname = attributes.match(/data-tagname="([^"]*)"/);
        tagname = tagname && tagname[1] || 'ons-button';
        if (href) {
            var clickable = tagname !== 'ons-button' ? ' class="clickable"' : '';
            if (href.split('/').length === 2 || href.endsWith('overview') || href.endsWith('overzicht')) { // root page
                return '<' + tagname + clickable + ' onclick="fn.load(\'' + href + '\')">' + content + '</' + tagname + '>';
            }
            else {
                return '<' + tagname + clickable + ' onclick="fn.navigator.pushPage(\'' + href + '\')">' + content + '</' + tagname + '>';
            }
        } else {
            return match;
        }
    }).replace(/src="\/([^"]*)"/gi, 'src="./$1"'); %>

